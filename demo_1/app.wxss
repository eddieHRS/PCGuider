/**icon-change**/

.icon-xiajiantou:after {
  font-family:"iconfont" !important;
  content: '\e733';
  font-size: 40rpx;
  position: absolute;
  top: 50%;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  right: 10rpx;
}
.icon-shangjiantou:after {
  font-family:"iconfont" !important;
  content: '\e734';
  font-size: 40rpx;
  position: absolute;
  top: 50%;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  right: 10rpx;
}
/**card-view**/
.card{
  align-items: center;
  padding: 20rpx;
  font-size: 28rpx;
  position: relative;
  overflow: hidden;
  margin-left: 5px;
  margin-right: 5px;
  background-color: #fff;
  background-clip: padding-box;
  box-shadow: 0 1px 2px rgba(0,0,0,.3);
}
.title{
  color: #868686;
}
.card-title{
  margin-left: 0px;
  margin-right: 0px;
  align-items: center;
  padding: 20rpx;
  font-size: 20rpx;
  color: #fff;
  position: relative;
  overflow: hidden;
  background-color:#c8c7cc92;
  background-clip: padding-box;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
  margin-bottom: 5px;
  text-align: center;
}
.card-tail{
  padding: 0 5px;
  line-height: 60rpx;
  font-size: 20rpx;
  position: relative;
  color:#c8c7cc;
  border-top: 1px solid #c8c7cc;
}
/**table**/
.table {
  line-height: 60rpx;
  font-size: 20rpx;
  position: relative;
  color:#c8c7cc;
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  height: 40rpx;
}
.td {
    width:40%;
    text-align: left;
    padding-top: 15rpx;
}
.td-pic {
    width:100%;
    text-align: left;
    padding-top: 15rpx;
}
.th {
  width: 40%;
  justify-content: center;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.tui-content{
  height: 100%;
  padding: 10px;
}
