/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.blackBG {
  align-items: center;
  font-size: 28rpx;
  padding-top: 60rpx;
  padding-bottom: 20rpx;
  position: relative;
  overflow: hidden;
  background-color: #000;
  background-clip: padding-box;
  box-shadow: 0 1px 2px rgba(0,0,0,.3);
}
